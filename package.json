{
  "dependencies": {
    "@types/node": "^14.14.11",
    "typescript": "^4.1.2"
  },
  "scripts": {
    "tsc": "./node_modules/.bin/tsc",
    "run10": "yarn tsc 10-result.ts && node 10-result.js",
    "run12": "yarn tsc 12-result.ts && node 12-result.js",

    "demo": "echo \"./node_modules/.bin/tsc$npm_config_file.ts | node $npm_config_file.js\"",
    "build": "./node_modules/.bin/tsc --p ./tsconfig.json",
    "teststart": "./node_modules/.bin/tsc$npm_config_file | node$npm_config_file",
    "check-env": "node -e 'console.log(process.env)' | grep npm"
  }
}
